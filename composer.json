{
  "name": "moneroo/moneroo-php",
  "description": "This is simple PHP Package to interact with Moneroo API.",
  "keywords": [
    "AxaZara",
    "laravel",
    "moneroo",
    "moneroo.io",
    "moneroo-laravel",
    "mobile-money",
    "payment"
  ],
  "homepage": "https://docs.moneroo.io/",
  "license": "MIT",
  "authors": [
    {
      "name": "Axa Zara Core Dev",
      "role": "Developer"
    }
  ],
  "contributors": [
    {
      "name": "Elias Elimah"
    }
  ],
  "require": {
    "php": "^7.4",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.7"
  },
  "require-dev": {
    "axazara/php-cs": "^1.0",
    "insolita/unused-scanner": "^2.4",
    "phpstan/phpstan-phpunit": "^1.3",
    "phpunit/phpunit": "^9.6",
    "roave/security-advisories": "dev-latest",
    "spatie/ray": "^1.32",
    "fakerphp/faker": "^1.23"
  },
  "autoload": {
    "psr-4": {
      "Moneroo\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Moneroo\\Tests\\": "tests"
    }
  },
  "scripts": {
    "analyse": "vendor/bin/phpstan analyse",
    "sniff": "vendor/bin/php-cs-fixer fix -vvv --dry-run --show-progress=dots",
    "format": "vendor/bin/php-cs-fixer fix -vv",
    "unused": "vendor/bin/unused_scanner unused-scanner.php",
    "test": [
      "Composer\\Config::disableProcessTimeout",
      "vendor/bin/phpunit --testdox"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
